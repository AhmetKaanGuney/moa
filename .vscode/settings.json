{
    "python.linting.pylintEnabled": false,
    "python.linting.enabled": true,
    "python.linting.pylamaEnabled": true,
    "code-runner.defaultLanguage": "python",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.detectLocale": "on",
    "python.formatting.provider": "black",
    // "python.formatting.provider": "black",
    // "editor.formatOnSave": true,
    // "python.formatting.blackArgs": [
    //     "--line-length",
    //     "119"
    // ],
    "editor.bracketPairColorization.enabled": true,
    "workbench.colorCustomizations": {
        "editorError.foreground": "#ff0000",
        "editorWarning.foreground": "#ff990075",
        "editorInfo.foreground": "#00aeff00",
        "editorBracketMatch.border": "#686868",
        "editorBracketMatch.background": "#2f2f2f",
    },
    "files.trimTrailingWhitespace": true,
    "liveServer.settings.multiRootWorkspaceName": "moa"
}